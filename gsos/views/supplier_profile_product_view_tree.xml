<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data>

      <record id="gsos_supplier_profile_product_view_tree" model="ir.ui.view">
         <field name="name">gsos.supplier.profile.product.tree</field>
         <field name="model">gsos.supplier.profile.product</field>
         <field name="arch" type="xml">
            <tree string="Product" editable="top">
               <field name="type_material" />
               <field name="description" />
               <field name="container_type" />
               <field name="ship_to_location" />
               <field name="tag" invisible="1" />
            </tree>
         </field>
      </record>

   </data>
</openerp>
