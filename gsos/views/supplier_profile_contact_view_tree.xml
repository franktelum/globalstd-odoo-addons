<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data>

      <record id="gsos_supplier_profile_contact_view_tree" model="ir.ui.view">
         <field name="name">gsos.supplier.profile.contact.tree</field>
         <field name="model">gsos.supplier.profile.contact</field>
         <field name="arch" type="xml">
            <tree string="Contacts" editable="top">
               <field name="name"/>
               <field name="job_position"/>
               <field name="phone"/>
               <field name="email" widget="email"/>
               <field name="tag" invisible="1" />
            </tree>
         </field>
      </record>

   </data>
</openerp>
