<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data>

      <record id="gsos_checklist_view_tree" model="ir.ui.view">
         <field name="name">gsos.checklist.tree</field>
         <field name="model">gsos.checklist</field>
         <field name="arch" type="xml">
            <tree string="Checklists" editable="top">
